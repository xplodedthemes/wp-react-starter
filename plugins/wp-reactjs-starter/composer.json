{
    "description": "WordPress Plugin Boilerplate using new web technologies.",
    "license": "GPL-3.0-or-later",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "metasyntactical/composer-plugin-license-check": true,
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "authors": [
        {
            "name": "Matthias GÃ¼nter"
        }
    ],
    "extra": {
        "metasyntactical/composer-plugin-license-check": {
            "whitelist": [
                "MIT",
                "ISC",
                "BSD-2-Clause",
                "BSD-3-Clause",
                "BSD",
                "Apache-2.0",
                "Apache2",
                "Artistic-2.0",
                "WTFPL",
                "CC-0",
                "CC0-1.0",
                "MPL-2.0",
                "ZLib",
                "Unlicense",
                "GPL-2.0",
                "GPL-2.0-or-later",
                "GPL-3",
                "GPL-3.0-or-later",
                "LGPL-3.0-or-later"
            ],
            "blacklist": [
                "*"
            ],
            "packages": [
                "ignore-packages-here@1.0.0"
            ]
        }
    },
    "require": {
        "wp-reactjs-multi-starter/utils": "@dev"
    },
    "require-dev": {
        "10up/wp_mock": "^0.4.2",
        "antecedent/patchwork": "^2.1.12",
        "comcast/php-legal-licenses": "^1.1.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "metasyntactical/composer-plugin-license-check": "^1.1.0",
        "php-stubs/wordpress-stubs": "^5.3.2",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpdocumentor/phpdocumentor": "^3.0",
        "phpunit/phpunit": "^9",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "slevomat/coding-standard": "^6.0@dev",
        "squizlabs/php_codesniffer": "^3.5",
        "wp-coding-standards/wpcs": "^2.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MatthiasWeb\\WPRJSS\\": [
                "inc/",
                "src/inc/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MatthiasWeb\\WPRJSS\\Test\\": [
                "test/phpunit/"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
